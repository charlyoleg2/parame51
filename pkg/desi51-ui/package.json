{
	"name": "desi51-ui",
	"description": "The sveltekit app for desi51",
	"version": "0.5.7",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame51.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame51/",
	"author": "charlyoleg2",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"scripts": {
		"cp_pgdsvg": "shx cp -r ../../node_modules/desi51/dist/pgdsvg static/",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"lint": "prettier --check . && eslint .",
		"ci": "run-s cp_pgdsvg build lint check test:unit:once",
		"format": "prettier --write .",
		"github-pages-fix": "touch build/.nojekyll",
		"clean": "shx rm -fr .svelte-kit build node_modules static/pgdsvg"
	},
	"dependencies": {
		"desi51": "^0.5.18",
		"geometrix": "^0.5.46",
		"geomui": "^0.5.48"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.20",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"sass": "^1.77.8",
		"shx": "^0.3.4",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.5",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"vitest": "^1.6.0"
	}
}
